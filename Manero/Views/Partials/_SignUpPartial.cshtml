<!-- Sign up -->
<section class="sign-up">
    <div class="container">
        <!-- Go back navbar -->
        <div class="go-back-navbar">
            <button class="first"><i class="fa-solid fa-chevron-left"></i></button>
            <div class="middle">Sign up</div>
            <div class="last">test</div>
        </div>

        <div class="sign-up-section">
            <div class="vertical-line"></div>
            <h1 class="sign-up-title">Sign Up</h1>
            <!-- Sign up form -->
            <form>
                <div class="input-grp">
                    <label>NAME</label>
                    <input type="email">
                    <i class="fa-solid fa-check"></i> <!-- <i class="fa-solid fa-xmark"></i> -->
                </div>
                <div class="input-grp">
                    <label>EMAIL</label>
                    <input type="email">
                    <i class="fa-solid fa-check"></i> <!-- <i class="fa-solid fa-xmark"></i> -->
                </div>
                <div class="input-grp">
                    <label>PASSWORD</label>
                    <input type="password">
                    <button><i class="fa-regular fa-eye-slash"></i></button> <!-- <i class="fa-regular fa-eye"></i>  -->
                </div>
                <div class="input-grp">
                    <label>CONFIRM PASSWORD</label>
                    <input type="password">
                    <button><i class="fa-regular fa-eye-slash"></i></button> <!-- <i class="fa-regular fa-eye"></i>  -->
                </div>

                <button class="button-theme">SIGN UP</button>
            </form>

            <div class="have-account">Already have an account? <a href="#">Sign in.</a></div>

            <div class="sign-up-with">
                <a href="#"><div class="facebook"><i class="fa-brands fa-facebook-f"></i></div></a>
                <a href="#"><div class="twitter"><i class="fa-brands fa-twitter"></i></div></a>
                <a href="#"><div class="google"><i class="fa-brands fa-google-plus-g"></i></div></a>
            </div>
        </div>
    </div>
</section>